{
  "name": "ruban",
  "version": "1.0.0",
  "private": true,
  "description": "鲁班前端",
  "scripts": {
    "start": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "prettier": "prettier -c --write \"src/**/*\"",
    "lint": "umi g tmp && npm run lint:js && npm run lint:prettier",
    "lint-staged:js": "eslint --fix --format=pretty",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-form": "^1.27.1",
    "@ant-design/pro-layout": "^6.19.3",
    "@ant-design/pro-table": "^2.56.0",
    "@ant-pro-go/config-use-request": "^1.2.0",
    "@ant-pro-go/styles": "^1.2.0",
    "@ant-pro-go/table": "^2.3.1",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-react": "^1.8.13",
    "ahooks": "^2.10.5",
    "antd": "^4.16.12",
    "classnames": "^2.2.6",
    "hox": "^1.1.6",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "named-urls": "^2.0.0",
    "rc-virtual-list": "^3.4.8",
    "react": "^17.0.2",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "umi": "^3.5.8",
    "zhihu-icons": "^1.14.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.165",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.0",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "2.7.0",
    "@umijs/yorkie": "^2.0.5",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "cli": {
    "ignoreFiles": [
      ".git",
      "CHANGELOG.md"
    ]
  }
}
